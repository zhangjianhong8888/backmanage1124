<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.qydata.dao.CustomerDeptAdminMapper">

    <!--插入数据-->
    <insert id="insertCustomerDeptAdmin" parameterType="CustomerDeptAdmin">
       insert into qyauth.CustomerDeptAdmin(customerId,deptNo,adminId,createTime)
       values(#{customerId},#{deptNo},#{adminId},sysdate())
    </insert>

    <insert id="insertCustomerDeptAdminTest" parameterType="CustomerDeptAdmin">
        insert into qyauth.CustomerDeptAdmin(customerId,deptNo,adminId,createTime)
        values(#{customerId},#{deptNo},#{adminId},sysdate())
    </insert>



</mapper>